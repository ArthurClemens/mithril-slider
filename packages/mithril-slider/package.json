{
  "name": "mithril-slider",
  "main": "dist/mithril-slider.js",
  "module": "dist/mithril-slider.mjs",
  "version": "0.4.1",
  "description": "Content Slider for Mithril on mobile and desktop",
  "scripts": {
    "build-main": "DEPS=0 ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.umd.js && DEPS=0 ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.es.js",
    "build-css": "BABEL_ENV=test babel-node scripts/build-css.js > dist/mithril-slider.css",
    "build": "npm run build-main && npm run build-css"
  },
  "dependencies": {
    "hammerjs": "^2.0.8",
    "mithril": "0.2.5"
  },
  "devDependencies": {
    "j2c": "1.0.0-4"
  },
  "keywords": [
    "slider",
    "carousel",
    "mobile",
    "mithril"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ArthurClemens/mithril-slider.git"
  },
  "author": "Arthur Clemens <arthurclemens@gmail.com> (http://visiblearea.com)",
  "license": "MIT"
}
